bool
CheckOpenGLBinds(const OpenGL *opengl)
{
	u8 *start = (u8*)opengl;
	u8 *end = start+sizeof(OpenGL);
	u8 *p = start;
	int fnptr_size = sizeof(void(*)(void));
	while(p < end) {
		if(*(void(**)(void))p == nullptr) {
			log("An OpenGL bind failed.");
			return false;
		}
		p += fnptr_size;
	}

	return true;
}

