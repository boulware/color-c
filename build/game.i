u32
RandomU32(u32 min, u32 max)
{
	if(min > max) return 0;
	if(min == max) return min;
	if(max == 0xFFFFFFFF)
	{
		max -= 1;
	}

	LCG &lcg = random::default_lcg;
	lcg.seed = (lcg.a * lcg.seed + lcg.c) % lcg.m;
	return (lcg.seed % (max-min+1)) + min;
}

