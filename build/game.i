		*f++ = '%';
		*f++ = 'd';
		*f++ = trait_chars[i];
		*f = '\0';

		int substring_length = snprintf(p, c::max_traitset_string_size, format_string, traits[i]);
		if(substring_length < 0 || substring_length > c::max_traitset_string_size)
		{
			
			traitset_string[0] = '\0';
			return traitset_string;
		}
		p += substring_length;

		prepend_space = true;
	}

	*p = '\0';
	return traitset_string;
}
